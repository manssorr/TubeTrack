{
  "name": "@tubetrack/server",
  "version": "0.1.0",
  "description": "Express server for TubeTrack with YouTube API proxy",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "type-check": "tsc --noEmit",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "test": "vitest",
    "test:run": "vitest run"
  },
  "dependencies": {
    "@tubetrack/shared": "file:../shared",
