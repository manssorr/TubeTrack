    "axios": "^1.6.8",
    "rate-limiter-flexible": "^5.0.3",
    "dotenv": "^16.4.5",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/node": "^20.11.30",
    "tsx": "^4.7.1",
    "vitest": "^1.4.0",
    "supertest": "^6.3.4",
    "@types/supertest": "^6.0.2"
  }
